{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 9000,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "xxx",
            "level": 0,
            "email": "your-email@example.com",
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": "caddy:80"
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "xtls",
        "xtlsSettings": {
          "alpn": ["h2", "http/1.1"],
          "certificates": [
            {
              "certificateFile": "/ssl/xxx/xxx.cer",
              "keyFile": "/ssl/xxx/xxx.key"
            }
          ]
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "policy": {
    "levels": {
      "0": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "bufferSize": 10240
      }
    }
  }
}
